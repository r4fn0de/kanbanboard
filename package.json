    "rust:clippy": "source ~/.cargo/env && cd src-tauri && cargo clippy -- -D warnings",
    "rust:clippy:fix": "source ~/.cargo/env && cd src-tauri && cargo clippy --fix --allow-dirty",
    "rust:test": "source ~/.cargo/env && cd src-tauri && cargo test",
